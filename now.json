{
  "name": "pkg",
  "version": 2,
  "alias": "pkg.nyc",
  "builds": [
    {"src": "pkg.js", "use": "@now/node"},
    {
      "src": "login.js",
      "use": "@now/node",
      "config": {"maxLambdaSize": "40mb"}
    },
    {"src": "package.json", "use": "@now/static-build"}
  ],
  "routes": [
    {"src": "^/pkg", "dest": "/pkg.js"},
    {"src": "^/login", "dest": "/login.js"}
  ]
}
