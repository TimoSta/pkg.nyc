{
  "name": "pkg",
  "version": 2,
  "alias": "pkg.nyc",
  "builds": [
    { "src": "pkg.js", "use": "@now/node" },
    { "src": "package.json", "use": "@now/static-build" }
  ],
  "routes": [{ "src": "^/pkg", "dest": "/pkg.js" }],
  "env": {
    "BUILDING_LINK_TOKEN": "@building-link-token",
    "BUILDING_LINK_PIN": "@building-link-pin"
  }
}
